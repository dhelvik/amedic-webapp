<!DOCTYPE html>
<html lang="en">
<% include head.ejs %>
<body>

<div class="container">
    <% include nav.ejs %>

    <div class="col-md-5">
        <div class="form-group">
            <label> ID: <%= result[0].ID %></label>
        </div>
        <div class="form-group">
            <label> Symptom ID: <%= result[0].symptoms_sheet_id %></label>
        </div>
        <div class="form-group">

            <label> Patient ID: <%= result[0].patient_id %></label>
        </div>
        <div class="form-group">

            <label> Diagnosis ID: <%= result[0].diagnosis_id %></label>
        </div>
        <div class="form-group">

            <label>User ID: <%= result[0].user_id %></label>
        </div>
        <div class="form-group">

            <label>Timestamp: <%= result[0].timestamp %></label>
        </div>


        <table>
            <tr>
                <th>Note</th>
                <th>Timestamp</th>
            </tr>
            <% notes = result[1];
            for(var i = 0; i < notes[i].length; i++){ %>
            <tr>
                <td><%= notes[i].ID %></td>
                <td><%= notes[i].description %></td>
            </tr>
            <% } %>

        </table>
    </div>
</div>
</body>
</html>